<html>
  <head>
    <title>My Angular App!</title>
    <script src="https://cdn.socket.io/socket.io-1.2.1.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.4/angular.min.js"></script>
    <script src="javascripts/application.js"></script>
    <script src="javascripts/services.js"></script>
    <script src="javascripts/directives.js"></script>
  </head>
  <body ng-app="notewithme" ng-controller="MainCtrl">
    <input eb-caret="textCursorPosition" ng-keypress="keypressed($event.keyCode)" ng-keyup="keypressedBackSpace($event.keyCode)">
    <div>

      {{message.nick}}<br/>
      {{message.position}}<br/>
      {{text}}
    </div>
    <canvas drawing></canvas>

  </body>
</html>